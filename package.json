{
  "name": "coiliiot_config_app",
  "version": "1.0.0",
  "description": "configuration app for data transfer unit",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run start:electron\" \"wait-on http://localhost:3000 && npm run start:react\"",
    "start:electron": "electron .",
    "start:react": "cd client && npm start",
    "build": "cd client && npm run build",
    "build-mac": "electron-packager . --overwrite --platform=darwin --icon=./icon.icns --arch=arm64 --prune=true --out=build",
    "create-dmg": "electron-installer-dmg ./build/coiliiot_config_app-darwin-arm64/coiliiot_config_app.app coiliiot_config_app --overwrite",
    "build-win": "electron-packager . --overwrite --platform=win32 --icon=./icon.ico --arch=x64 --prune=true --out=build",
    "create-installer": "node createWinInstaller.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruby94412/coiliiot_config_app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ruby94412/coiliiot_config_app/issues"
  },
  "homepage": "https://github.com/ruby94412/coiliiot_config_app#readme",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "electron-installer-dmg": "^4.0.0",
    "electron-packager": "^17.1.1",
    "electron-winstaller": "^5.1.0",
    "electron-wix-msi": "^5.0.0",
    "wait-on": "^7.0.1"
  }
}
